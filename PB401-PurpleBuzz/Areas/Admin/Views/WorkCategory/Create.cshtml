@using PB401_PurpleBuzz.Areas.Admin.Models.WorkCategory
@model WorkCategoryCreateVM

<form method="post" asp-action="create" asp-controller="workcategory">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" placeholder="Enter title" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>